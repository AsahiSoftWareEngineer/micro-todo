{
  "name": "microtodo-electron",
  "private": true,
  "version": "0.0.0",
  "main": "dist-electron/main.js",
  "scripts": {
    "dev": "NODE_ENV=development concurrently \"npm:dev:vite\" \"npm:dev:electron\"",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "dev:vite": "vite",
    "dev:electron": "wait-on http://localhost:5173 && electron -r ts-node/register ./dist-electron/main.js",
    "build:electron": "tsc -p tsconfig.electron.json"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@radix-ui/react-dialog": "^1.1.15",
